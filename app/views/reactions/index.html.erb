<div class="page-header">
  <h1>
    Reactions
    <a href="/reactions/new" class="btn btn-lg btn-success">New Reaction</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Reaction</th>
        <th>Status</th>
        <th>User</th>
        <th>Actions</th>
      </tr>

      <% @reactions.each do |reaction| %>
      <tr>
        <td><%= reaction.reaction %></td>
        <td><%= reaction.status_id %></td>
        <td><%= reaction.user_id %></td>
        <td>
          <a href="/reactions/<%= reaction.id %>" class="btn btn-primary">Show</a>
          <a href="/reactions/<%= reaction.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_reaction/<%= reaction.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
